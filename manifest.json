{
    "name": "Gamepad tester",
    "version": "0.01",
    "manifest_version": 3,
    "description": "A small extension that will help test the gamepad",
    "background":
    {
        "service_worker": "content.js"
    },
    "icons":
    {
        "16": "icon/icon16.png",
        "32": "icon/icon32.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "action":
    {
        "default_popup": "popup.html",
        "default_title": "GT"
    }
}
